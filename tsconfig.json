{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node",
    "allowJs": true,
    "lib": ["dom", "es2015.promise", "es5"]
  },
  "include": ["src/**/*"]
}
